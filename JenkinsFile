pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building SCM'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing Build code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the changes'
            }
        }
        stage('Release') {
            steps {
                echo 'Release'
            }
        }
    }
}
